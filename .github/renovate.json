{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:base", ":disableDependencyDashboard"],
	"addLabels": ["dependencies"],
	"automergeType": "branch",
	"rangeStrategy": "bump",
	"packageRules": [
		{
			"matchUpdateTypes": ["minor", "patch", "pin", "digest"],
			"automerge": true
		}
	]
}
